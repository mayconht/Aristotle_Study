# SonarQube Environment Configuration
# Copy this file to .env and customize the values

# SonarQube Configuration
SONAR_HOST_URL=http://localhost:9000
SONAR_PROJECT_KEY=aristotle-study
SONAR_PROJECT_NAME=Aristotle Study - User Management API
SONAR_PROJECT_VERSION=1.0

# SonarQube Authentication (generate a token in SonarQube UI: Administration > Security > Users)
# Leave empty to use default admin/admin credentials
SONAR_TOKEN=

# Database Configuration for SonarQube
POSTGRES_USER=sonar
POSTGRES_PASSWORD=sonar
POSTGRES_DB=sonar

# Coverage Configuration
COVERAGE_THRESHOLD=80
